{
  "manifest_version": 2,

  "name": "hub-marker",
  "description": "Bookmark github repos in a nice interface",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
    "https://api.github.com/"
  ]
}
